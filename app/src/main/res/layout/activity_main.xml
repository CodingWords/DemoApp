        app:layout_constraintTop_toTopOf="@+id/iv_icon_social_team_black"
        android:background="@drawable/circle"
        android:text="2"
        android:textColor="@color/colorWhite"
        android:layout_width="25dp"
        android:layout_height="25dp"
